<div id="app-content" class="col-xs-12 col-sm-10">
    <div class="row">
    	<div id="app-content-header">
			<ol class="breadcrumb">
				<li><a href="#dashboard"><i class="fa fa-home fa-2x"></i></a></li>
				<li><a href="">서비스관리</a></li>
				<li><a href="">분석</a></li>
				<li><a href="#06svWeekBsnSch/list">{{view.title}}</a></li>
			</ol>
		</div>
    </div>

	<div class="row">
        <div class="content-area-container col-xs-12">
            <div data-content-box-layer data-box-title="view.title">
                <!-- 주간업무일지 -->
                <div id="salesContent">
                	<div class="box-content-article">
						<div class="article-header">
							<div class="col-xs-6 article-header-box">
								<div class="info">{{svMainWeekBsnVO.title}}</div>
							</div>
							<div class="col-xs-6 article-header-box ta-r grid-export">
							</div>
						</div>
						<div data-ui-grid="svMainWeekBsnVO.grid" class="article-view grid" style="height: 80px;"
						     ui-grid-auto-resize ui-grid-resize-columns></div>
					</div>
					<div class="box-content-article">     
						<div class="article-header">
							<div class="col-xs-6 article-header-box">
								<div class="info"></div>
							</div>
							<div class="col-xs-6 article-header-box ta-r grid-export">
								<button type="button" class="ae-btn ae-download-btn" title="Excel다운로드" data-ng-click="svWeekBsnVO_0.downloadExcel()">
									<i class="fa fa-download"></i> Excel다운로드
								</button>
								<button type="button" class="ae-btn" title="Field설정" data-ng-click="svWeekBsnVO_0.setColumn()">
									<i class="fa fa-cog"></i> Column설정
								</button>
							</div>
						</div>
						<div class="article-header">
							<div class="col-xs-6 article-header-box">
								<div class="info">{{svWeekBsnVO_0.title}}</div>
							</div>
						</div>
						<div data-ng-if="svWeekBsnVO_0.grid.data.length > 0">
							<div ng-style="view.getTableHeight(0)" style="overflow-y:hidden;" data-ui-grid="svWeekBsnVO_0.grid" class="article-view grid"
						     ui-grid-auto-resize ui-grid-resize-columns></div>
                        </div>
                        <div class="article-header">
							<div class="col-xs-6 article-header-box">
								<div class="info">{{svWeekBsnVO_1.title}}</div>
