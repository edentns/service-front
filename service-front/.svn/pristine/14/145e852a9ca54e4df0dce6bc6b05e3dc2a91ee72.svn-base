<div class="form-type1">
	<table>
		<colgroup>
			<col style="width: 9%" />
			<col style="width: 16%" />
			<col style="width: 9%" />
			<col style="width: 16%" />
			<col style="width: 9%" />
			<col style="width: 16%" />
			<col style="width: 9%" />
			<col style="width: 16%" />
		</colgroup>
		<tbody>
		<tr>
			<th>수행번호</th>
			<td>{{vm.NO_SERV_PFM.value}}</td>
			<th>계획번호</th>
			<td>{{vm.NO_SERV_PLN.value}}</td>
			<th>계획시작<br/>일시</th>
			<td>{{vm.DTS_PLN_STRT.value}}</td>
			<th>계획종료<br/>일시</th>
			<td>{{vm.DTS_PLN_END.value}}</td>
		</tr>
		<tr>
			<th>출발일시</th>
			<td>
				<div>{{vm.DTS_PFM_STRT.value}}</div>
			</td>
			<th>도착일시</th>
			<td>
				<div>{{vm.DTS_PFM_ARV.value}}</div>
			</td>
			<th>확인일시</th>
			<td>
				<div>{{vm.DTS_PFM_CFM.value}}</div>
			</td>
			<th>해결(종료)<br/>일시</th>
			<td>
				<div>{{vm.DTS_PFM_SOL.value}}</div>
			</td>
		</tr>
		<tr>
			<th>처리 후 <br/>보고방식</th>
			<td>
				<div>{{vm.NM_PROC_AFT_RPT_WAY.value}}</div>
			</td>
			<th>수행인원</th>
			<td colspan="5">
				<div data-ng-repeat="pfmr in vm.PFMR_LIST.compositeData" class="tags" style="margin-right: 3px;">
					<span class="font-sm gray-light">{{pfmr.NM_DEPT}}</span>
					<span class="font-w700">{{pfmr.NM_EMP}}</span>
				</div>
			</td>
		</tr>
		<tr>
			<th>증상</th>
			<td colspan=7>
				<div class="textarea-wrapper">
					<div data-ng-bind-html="vm.DC_STAT.value|unsafe"></div>
				</div>
			</td>
		</tr>
		<tr>
			<th>원인</th>
			<td colspan=7>
				<div class="textarea-wrapper">
					<div data-ng-bind-html="vm.DC_CUS.value|unsafe"></div>
				</div>
			</td>
		</tr>
		<tr>
			<th>조치</th>
			<td colspan=7>
				<div class="textarea-wrapper">
					<div data-ng-bind-html="vm.DC_PROC.value|unsafe"></div>
				</div>
			</td>
		</tr>
		<tr>
			<th>대책</th>
			<td colspan=7>
				<div class="textarea-wrapper">
					<div data-ng-bind-html="vm.DC_CTM.value|unsafe"></div>
				</div>
			</td>
		</tr>
		<tr>
			<th>HPRC<br>CASE#1</th>
			<td>{{vm.DC_HPRC_CASE_1.value}}</td>
			<th>HPRC<br>CASE#2</th>
			<td>{{vm.DC_HPRC_CASE_2.value}}</td>
			<th>HPRC<br>CASE#3</th>
			<td>{{vm.DC_HPRC_CASE_3.value}}</td>
			<th></th>
			<td></td>
		</tr>
		<tr>
			<th>파일정보</th>
			<td colspan="7">
				<ui-file-view data-files="vm.PFM_FILE_LIST"></ui-file-view>
			</td>
		</tr>
		</tbody>
	</table>
</div>