<div id="sidebar-left" class="col-xs-2 col-sm-2">
	<ul class="nav main-menu">
		<li data-ng-repeat="depth1 in depthList" data-ng-class="{'dropdown': depth1.child.length > 0}">
			<a data-ng-if="depth1.child.length == 0" data-ui-sref="{{depth1.path}}" data-ui-sref-opts="{reload: true}" data-ng-class="{'active': isActive({{'depth1.url'}}, 0)}">
				<i class="fa {{depth1.className}}"></i>
				<span>{{depth1.name}}</span>
			</a>
			<a data-ng-if="depth1.child.length > 0" href="#" class="dropdown-toggle">
				<i class="fa {{depth1.className}}"></i>
				<span>{{depth1.name}}</span>
			</a>
			<ul data-ng-if="depth1.child.length > 0" class="dropdown-menu">
				<li data-ng-repeat="depth2 in depth1.child" data-ng-class="{'dropdown': depth2.child.length > 0}">
					<a data-ng-if="depth2.child.length == 0" data-ui-sref="{{depth2.path}}" data-ui-sref-opts="{reload: true}" data-ng-class="{'active': isActive({{'depth2.url'}}, 0)}">
						<span>{{depth2.name}}</span>
					</a>
					<a data-ng-if="depth2.child.length > 0" href="#" class="dropdown-toggle">
						<span>{{depth2.name}}</span>
					</a>
					<ul data-ng-if="depth2.child.length > 0" class="dropdown-menu">
						<li data-ng-repeat="depth3 in depth2.child" data-ng-class="{'dropdown': depth3.child.length > 0}">
							<a data-ng-if="depth3.child.length == 0" data-ui-sref="{{depth3.path}}" data-ui-sref-opts="{reload: true}" data-ng-class="{'active': isActive({{'depth3.url'}}, 0)}">
								<span>&nbsp;&nbsp;&nbsp;&nbsp;{{depth3.name}}</span>
							</a>
							<a data-ng-if="depth3.child.length > 0" href="#" class="dropdown-toggle">
								<span>&nbsp;&nbsp;&nbsp;&nbsp;{{depth3.name}}</span>
							</a>
							<ul data-ng-if="depth3.child.length > 0" class="dropdown-menu">
								<a data-ui-sref="{{depth4.path}}" data-ui-sref-opts="{reload: true}" data-ng-class="{active: isActive({{'depth4.url'}}, 0)}">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{{depth4.name}}</a>
							</ul>
						</li>
					</ul>
				</li>
			</ul>
		</li>
	</ul>
</div>