<header class="navbar">
	<div class="container-fluid expanded-panel">
		<div class="row">
			<div id="logo" class="col-xs-2">
				<a href>SLM</a>
			</div>
			<div id="top-panel" class="col-xs-10">
				<div class="row">
					<div class="col-xs-4">
						<a href="#" class="show-sidebar">
							<i class="glyphicon glyphicon-transfer fa-lg" style="margin-right: 10px;"></i>
						</a>
					</div>
					<div class="col-xs-8 top-panel-right">
						<ul class="nav navbar-nav pull-right panel-menu" dropdown>
							<li class="dropdown">
								<a href class="dropdown-toggle account" style="line-height: 48px;" dropdown-toggle data-ng-click="headerIconVO.toggleDropdown($event)">
									<div class="avatar">
										<i class="fa fa-user"></i>
									</div>
									<i class="fa fa-angle-down pull-right"></i>
									<div class="user-mini pull-right">
										<span>
											<span class="header-departNm">{{webApp.user.DEPT_NAME}}</span>
											{{webApp.user.NAME}}
										</span>
									</div>
								</a>
								<ul data-ng-show="headerIconVO.isShowDropdown" class="dropdown-menu" style="display:block;">
									<li>
										<a href="#" data-ng-click="headerIconVO.moveProfile($event)">
											<i class="fa fa-user"></i>
											<span>Profile</span>
										</a>
									</li>
									<li>
										<a href="#" data-ng-click="headerIconVO.doLogout($event)">
											<i class="fa fa-power-off"></i>
											<span>Logout</span>
										</a>
									</li>
								</ul>
							</li>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</div>
</header>
