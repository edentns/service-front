<div data-ng-controller="CoBusiPrscCtrl">
    <div class="box-content-article">
        <div class="article-view">
              <!-- CHART -->
              <div data-ng-show="busiPrsc.tabName == 'CHART'" class="jui-chart">
                  <form class="box-content-search">
                      <div data-inline-period-search data-ng-model="busiPrsc.chart.search.period" data-change="busiPrsc.find()" data-is-execute="false"></div>
                  </form>
                  <div class="row jui-chart-row">
                      <div class="jui-bar-chart jui-bar-radio-group">
                          <div class="radio" data-ng-repeat="dept in busiPrsc.chart.search.deptList">
                              <label>
                                  <input type="radio" data-ng-model="busiPrsc.chart.search.selected" data-ng-value="dept.value" data-ng-change="busiPrsc.chart.search.changeSelectedData()">
                                  {{dept.name}}
                              </label>
                          </div>
                      </div>
                      <div class="chart" jui-chart="pie" data-jui-config="busiPrsc.chart.config"></div>
                  </div>
              </div>

              <!-- TABLE -->
              <div data-ng-show="busiPrsc.tabName == 'TABLE'">
                  <form class="box-content-search">
                      <div data-inline-period-search data-ng-model="busiPrsc.table.search.period" data-change="busiPrsc.find()" data-is-execute="false"></div>
                  </form>
                  <div class="tbl-type1 mt-5">
                      <table>
                          <thead>
                          <tr>
                              <th scope="col">부서(이름)</th>
                              <th scope="col">합계</th>
                              <th scope="col">수주등록</th>
                              <th scope="col">수주진행</th>
                              <th scope="col">수주완료</th>
                              <th scope="col">수주실패</th>
                              <th class="last" scope="col">수주취소</th>
                          </tr>
                          </thead>

                          <tfoot data-ng-if="busiPrsc.auth.manager">
                          <tr>
                              <th scope="row">합계</th>
                              <td>{{busiPrsc.table.total.totalCnt | number}}</td>
                              <td>{{busiPrsc.table.total.signupCnt | number}}</td>
                              <td>{{busiPrsc.table.total.progressCnt | number}}</td>
                              <td>{{busiPrsc.table.total.completionCnt | number}}</td>
                              <td>{{busiPrsc.table.total.failCnt | number}}</td>
                              <td class="last">{{busiPrsc.table.total.cancelCnt | number}}</td>
                          </tr>
                          </tfoot>

                          <tbody>
                          <tr data-ng-repeat="busiPrsc in busiPrsc.table.data">
                              <td>
                                  <span>{{busiPrsc.DEPT_NAME}}</span>
                              </td>
                              <td>{{busiPrsc.totalCnt | number}}</td>
                              <td>{{busiPrsc.signupCnt | number}}</td>
                              <td>{{busiPrsc.progressCnt | number}}</td>
                              <td>{{busiPrsc.completionCnt | number}}</td>
                              <td>{{busiPrsc.failCnt | number}}</td>
                              <td class="last">{{busiPrsc.cancelCnt | number}}</td>
                          </tr>
                          </tbody>
                      </table>
                  </div>
              </div>
        </div>
    </div>
</div>
